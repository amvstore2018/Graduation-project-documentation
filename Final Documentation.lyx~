\end_layout

\end_inset


\end_layout

\begin_layout Section
Linearization
\end_layout

\begin_layout Standard
Assume 
\end_layout

\begin_layout Itemize
Zero initial conditions
\end_layout

\begin_layout Itemize
Small angle approximation
\end_layout

\begin_deeper
\begin_layout Itemize

\family roman
\series medium
\shape up
\size normal
\emph off
\bar no
\strikeout off
\uuline off
\uwave off
\noun off
\color none
\begin_inset Formula $C_{\theta}\approxeq1$
\end_inset


\end_layout

\begin_layout Itemize

\family roman
\series medium
\shape up
\size normal
\emph off
\bar no
\strikeout off
\uuline off
\uwave off
\noun off
\color none
\begin_inset Formula $S_{\theta}\approxeq\triangle\theta$
\end_inset


\end_layout

\begin_layout Itemize

\family roman
\series medium
\shape up
\size normal
\emph off
\bar no
\strikeout off
\uuline off
\uwave off
\noun off
\color none
\begin_inset Formula $T_{\theta}\approxeq\triangle\theta$
\end_inset


\end_layout

\end_deeper
\begin_layout Itemize
Thrust and torque constants are equal to 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\begin{array}{cc}
\bar{K}=\frac{\stackrel[i=1]{n}{\sum K_{i}}}{n}, & \bar{B}=\frac{\stackrel[i=1]{n}{\sum B_{i}}}{n}\end{array}
\]

\end_inset


\end_layout

\begin_layout Paragraph
Force equation in Z direction
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\because Z+mgC_{\theta}C_{\Phi}=m(\dot{w}+pv-qu)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\because Z=Z_{0}+\Delta Z
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\therefore Z=-mgC_{\theta}C_{\Phi}+\stackrel[i=1]{n}{\sum K_{i}}PWM_{i}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\stackrel[i=1]{n}{\bar{K}\sum}PWM_{i}=m(\dot{w}+pv-qu)
\]

\end_inset


\end_layout

\begin_layout Standard
Using Small disturbance theory
\begin_inset CommandInset citation
LatexCommand cite
key "Nelson"

\end_inset

 and checking our previous assumptions (Zero initial conditions)
\end_layout

\begin_layout Standard
\begin_inset Formula $pv=\left(p_{0}+\triangle p\right)\left(v_{0}+\triangle v\right)=p_{0}*v_{0}+v_{0}*\triangle p+p_{0}*\triangle v=0$
\end_inset


\end_layout

\begin_layout Standard
likewise 
\family roman
\series medium
\shape up
\size normal
\emph off
\bar no
\strikeout off
\uuline off
\uwave off
\noun off
\color none

\begin_inset Formula $qu=0$
\end_inset


\end_layout

\begin_layout Standard

\family roman
\series medium
\shape up
\size normal
\emph off
\bar no
\strikeout off
\uuline off
\uwave off
\noun off
\color none
\begin_inset Formula $\stackrel[i=1]{n}{\sum K_{i}}PWM_{i}=\bar{K}*\stackrel[i=1]{n}{\sum}PWM_{i}$
\end_inset


\end_layout

\begin_layout Standard
Let 
\begin_inset Formula $\delta_{Col}=\stackrel[i=1]{n}{\sum}PWM_{i}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\dot{w}=\frac{\bar{K}}{m}\delta_{Col}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\because a_{z}=\dot{w}+pv-qu=\dot{w}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\therefore\ddot{z}=\dot{w}=\frac{\bar{K}}{m}\delta_{Col}
\]

\end_inset


\end_layout

\begin_layout Standard
By taking laplace transform
\end_layout

\begin_layout Standard
\align center
\begin_inset Box ovalbox
position "t"
hor_pos "c"
has_inner_box 1
inner_pos "t"
use_parbox 0
use_makebox 0
width "100col%"
special "none"
height "1in"
height_special "totalheight"
thickness "0.4pt"
separation "3pt"
shadowsize "4pt"
framecolor "black"
backgroundcolor "none"
status collapsed

\begin_layout Plain Layout
\begin_inset Formula 
\[
\frac{Z\left(s\right)}{\delta_{Col}\left(s\right)}=\frac{\bar{K}}{mS^{2}}
\]

\end_inset


\end_layout

\end_inset


\end_layout

\begin_layout Paragraph
Moment equation in X direction
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\because L=I_{x}\dot{p}-I_{xz}\dot{r}+qr(I_{z}-I_{y})-I_{xz}pq
\]

\end_inset


\end_layout

\begin_layout Standard

\family roman
\series medium
\shape up
\size normal
\emph off
\bar no
\strikeout off
\uuline off
\uwave off
\noun off
\color none
\begin_inset Formula 
\[
\because L=L_{0}+\Delta L
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $L=-m*g*Length_{RG}*\cos\left(\theta\right)*\sin\left(\phi\right)-\nu*p-\kappa*\phi+d*\stackrel[i=1]{n/2}{\sum}\left(-1\right)^{i+1}K_{2i}PWM_{2i}$
\end_inset


\end_layout

\begin_layout Standard
Let 
\begin_inset Formula $\delta_{Lat}=\stackrel[i=1]{n/2}{\sum}\left(-1\right)^{i+1}PWM_{2i}$
\end_inset


\end_layout

\begin_layout Standard
Like the previous example, we can simplify it to
\end_layout

\begin_layout Standard
\begin_inset Formula $L=-\left(m*g*Length_{RG}+\kappa\right)\triangle\phi-\nu*\triangle p+d*\bar{K}*\delta_{Lat}$
\end_inset


\end_layout

\begin_layout Standard

\family roman
\series medium
\shape up
\size normal
\emph off
\bar no
\strikeout off
\uuline off
\uwave off
\noun off
\color none
\begin_inset Formula $qr=0$
\end_inset

 and 
\begin_inset Formula $I_{xz}=0$
\end_inset


\end_layout

\begin_layout Standard
for simplicity let 
\family roman
\series medium
\shape up
\size normal
\emph off
\bar no
\strikeout off
\uuline off
\uwave off
\noun off
\color none

\begin_inset Formula $\triangle\phi=\phi,\,\triangle p=p$
\end_inset

 and we know that these quantities are deltas of the original values
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\therefore I_{x}\dot{p}=-\left(m*g*Length_{RG}+\kappa\right)\phi-\nu*p+d*\bar{K}*\delta_{Lat}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\because\ddot{\phi}=\dot{p}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\therefore I_{x}\ddot{\phi}=-\left(m*g*Length_{RG}+\kappa\right)\phi-\nu*\dot{\phi}+d*\bar{K}*\delta_{Lat}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\therefore I_{x}\ddot{\phi}+\nu*\dot{\phi}+\left(m*g*Length_{RG}+\kappa\right)\phi=d*\bar{K}*\delta_{Lat}
\]

\end_inset


\end_layout

\begin_layout Standard
By taking laplace transform
\end_layout

\begin_layout Standard

\family roman
\series medium
\shape up
\size normal
\emph off
\bar no
\strikeout off
\uuline off
\uwave off
\noun off
\color none
\begin_inset Formula 
\[
\frac{\phi\left(s\right)}{\delta_{Lat}\left(s\right)}=\frac{d*\bar{K}}{I_{x}S^{2}+\nu*S+\left(m*g*Length_{RG}+\kappa\right)}\,(rad/PWM)
\]

\end_inset


\end_layout

\begin_layout Standard
\align center
\begin_inset Box ovalbox
position "t"
hor_pos "c"
has_inner_box 1
inner_pos "t"
use_parbox 0
use_makebox 0
width "100col%"
special "none"
height "1in"
height_special "totalheight"
thickness "0.4pt"
separation "3pt"
shadowsize "4pt"
framecolor "black"
backgroundcolor "none"
status open

\begin_layout Plain Layout
\begin_inset Formula 
\[
\frac{\phi\left(s\right)}{\delta_{Lat}\left(s\right)}=\frac{d*\bar{K}}{I_{x}S^{2}+\nu*S+\left(m*g*Length_{RG}+\kappa\right)}*\frac{180}{\pi}\,(deg/PWM)
\]

\end_inset


\end_layout

\end_inset


\end_layout

\begin_layout Paragraph
Moment equation in Y direction
\end_layout

\begin_layout Standard
As the system is symmetric in both structure and load, the transfer function
 is the same.
\end_layout

\begin_layout Standard
\align center
\begin_inset Box ovalbox
position "t"
hor_pos "c"
has_inner_box 1
inner_pos "t"
use_parbox 0
use_makebox 0
width "100col%"
special "none"
height "1in"
height_special "totalheight"
thickness "0.4pt"
separation "3pt"
shadowsize "4pt"
framecolor "black"
backgroundcolor "none"
status open

\begin_layout Plain Layout
\begin_inset Formula 
\[
\frac{\theta\left(s\right)}{\delta_{Long}\left(s\right)}=\frac{d*\bar{K}}{I_{y}S^{2}+\nu*S+\left(m*g*Length_{RG}+\kappa\right)}*\frac{180}{\pi}\,(deg/PWM)
\]

\end_inset


\end_layout

\end_inset


\end_layout

\begin_layout Standard
Let 
\begin_inset Formula $\delta_{Long}=\stackrel[i=1]{n/2+1}{\sum}\left(-1\right)^{i+1}PWM_{2i-1}$
\end_inset


\end_layout

\begin_layout Paragraph
Moment equation in Z direction
\end_layout

\begin_layout Standard

\family roman
\series medium
\shape up
\size normal
\emph off
\bar no
\strikeout off
\uuline off
\uwave off
\noun off
\color none
\begin_inset Formula 
\[
\because N=-I_{xz}\dot{p}+I_{z}\dot{r}+pq(I_{y}-I_{x})+I_{xz}qr
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\because N=N_{0}+\Delta N
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $N=-\nu*r+\stackrel[i=1]{n/2}{\sum}B_{2i}*PWM_{2i}-\stackrel[i=1]{n/2+1}{\sum}B_{2i-1}*PWM_{2i-1}$
\end_inset


\end_layout

\begin_layout Standard
Let 
\begin_inset Formula $\delta_{Pend}=\stackrel[i=1]{n/2+1}{\sum}PWM_{2i-1}-\stackrel[i=1]{n/2}{\sum}PWM_{2i}$
\end_inset


\end_layout

\begin_layout Standard
Like the previous example, we can simplify it to
\end_layout

\begin_layout Standard
\begin_inset Formula $N=-\nu*\triangle r+\bar{B}*\delta_{Pend}$
\end_inset


\end_layout

\begin_layout Standard

\family roman
\series medium
\shape up
\size normal
\emph off
\bar no
\strikeout off
\uuline off
\uwave off
\noun off
\color none
\begin_inset Formula $pq=0$
\end_inset

 and 
\begin_inset Formula $I_{xz}=0$
\end_inset


\end_layout

\begin_layout Standard
for simplicity let 
\family roman
\series medium
\shape up
\size normal
\emph off
\bar no
\strikeout off
\uuline off
\uwave off
\noun off
\color none

\begin_inset Formula $\triangle\psi=\psi,\,\triangle r=r$
\end_inset

 and we know that these quantities are deltas of the original values
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\therefore I_{z}\dot{r}=-\nu*r+\bar{B}*\delta_{Pend}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\because\ddot{\psi}=\dot{r}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\therefore I_{z}\ddot{\psi}=-\nu*\dot{\psi}+\bar{B}*\delta_{Pend}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\therefore I_{z}\ddot{\psi}+\nu*\dot{\psi}=\bar{B}*\delta_{Pend}
\]

\end_inset


\end_layout

\begin_layout Standard
By taking laplace transform
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\psi\left(s\right)}{\delta_{Pend}\left(s\right)}=\frac{\bar{B}}{I_{z}S^{2}+\nu*S}\,(rad/PWM)
\]

\end_inset


\end_layout

\begin_layout Standard
\align center
\begin_inset Box ovalbox
position "t"
hor_pos "c"
has_inner_box 1
inner_pos "t"
use_parbox 0
use_makebox 0
width "100col%"
special "none"
height "1in"
height_special "totalheight"
thickness "0.4pt"
separation "3pt"
shadowsize "4pt"
framecolor "black"
backgroundcolor "none"
status collapsed

\begin_layout Plain Layout
\begin_inset Formula 
\[
\frac{\psi\left(s\right)}{\delta_{Pend}\left(s\right)}=\frac{\bar{B}}{I_{z}S^{2}+\nu*S}*\frac{180}{\pi}\,(deg/PWM)
\]

\end_inset


\end_layout

\end_inset


\end_layout

