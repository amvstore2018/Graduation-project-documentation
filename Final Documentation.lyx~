\begin_layout Chapter
Numerical Solution of differential equations using Runge-Kutta 4th order
 
\begin_inset CommandInset label
LatexCommand label
name "chap:Numerical-Solution-of"

\end_inset


\end_layout

\begin_layout Paragraph
Runge-Kutta 4th order
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\begin{array}{c}
k_{1}=h\,f\left(x_{n},y_{n}\right)\\
k_{2}=h\,f\left(x_{n}+\frac{h}{2},y_{n}+\frac{k_{1}}{2}\right)\\
k_{3}=h\,f\left(x_{n}+\frac{h}{2},y_{n}+\frac{k_{2}}{2}\right)\\
k_{4}=h\,f\left(x_{n}+h,y_{n}+k_{3}\right)\\
y_{n+1}=y_{n}+\frac{1}{6}\left(k_{1}+2k_{2}+2k_{3}+k_{4}\right)
\end{array}
\]

\end_inset


\end_layout

\begin_layout Paragraph
Flow Chart
\end_layout

\begin_layout Standard
\align center
\begin_inset Float figure
wide false
sideways false
status collapsed

\begin_layout Plain Layout
\align center
\begin_inset Graphics
	filename Figures/Appendix/flowChart.eps
	lyxscale 50
	width 10cm

\end_inset


\end_layout

\begin_layout Plain Layout
\begin_inset Caption Standard

\begin_layout Plain Layout
RK4 flow chart
\end_layout

\end_inset


\end_layout

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Newpage pagebreak
\end_inset


\end_layout

\begin_layout Section
Mat-lab code Implementation
\end_layout

\begin_layout Standard
\begin_inset CommandInset include
LatexCommand lstinputlisting
filename "D:/Graduation project/Matlab-project-files/Non linear simulation/RK4.m"
lstparams "language=Matlab,keywordstyle={\\color{blue}},commentstyle={\\color{magenta}\\itshape},emphstyle={\\color{red}},basicstyle={\\ttfamily},stringstyle={\\color{green}},identifierstyle={\\color{cyan}},label={Runge Kutta 4th order}"

\end_inset


\end_layout

\begin_layout Paragraph
Testing
\end_layout

\begin_layout Standard
we compared the output of this function with the analytical solution of
 this differential equation
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
t^{2}\frac{d^{2}y}{dt^{2}}-2t\frac{dy}{dt}+2y=t^{3}\log\left(t\right)
\]

\end_inset


\end_layout

\begin_layout Standard
the analytical solution of the previous differential equation is 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\begin{array}{cc}
Y\left(t\right)=\frac{7}{4}*t+\frac{t^{3}}{2}*\log\left(t\right)-\frac{3}{4}*t^{3} & for\end{array}1\leq t\leq2
\]

\end_inset


\end_layout

\begin_layout Standard
where 
\begin_inset Formula $y'\left(1\right)=0$
\end_inset

 , 
\begin_inset Formula $y\left(1\right)=1$
\end_inset


\end_layout

\begin_layout Paragraph
Solution procedure
\end_layout

\begin_layout Standard
first write the second order differential equation as a system of first
 order differential equations
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
let\begin{array}{c}
y_{1}=y\\
y_{2}=y'
\end{array}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\therefore\begin{array}{c}
y_{1}'=y_{2}\\
y_{2}'=\frac{2y_{2}}{t}-\frac{2y_{1}}{t^{2}}+t\log\left(t\right)
\end{array}
\]

\end_inset


\end_layout

\begin_layout Paragraph
Function Mat-lab code
\end_layout

\begin_layout Standard
\begin_inset CommandInset include
LatexCommand lstinputlisting
filename "D:/Graduation project/Matlab-project-files/Numerical integration/f.m"
lstparams "language=Matlab,keywordstyle={\\color{blue}},commentstyle={\\color{magenta}\\itshape},emphstyle={\\color{red}},basicstyle={\\ttfamily},stringstyle={\\color{green}},identifierstyle={\\color{cyan}},label={system of first order differianl equations}"

\end_inset


\end_layout

\begin_layout Paragraph
Testing Runge-kutta Mat-lab code
\end_layout

\begin_layout Standard
